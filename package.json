{
  "name": "vscode-css-variables",
  "version": "1.2.1",
  "description": "",
  "publisher": "vunguyentuan",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:vue-html",
    "onLanguage:scss",
    "onLanguage:typescriptreact",
    "onLanguage:less",
    "onLanguage:css",
    "onLanguage:vue-postcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vunguyentuan/vscode-css-variables.git"
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "contributes": {
    "configuration": {
      "title": "CSS Variables",
      "properties": {
        "cssVariables.lookupFiles": {
          "type": [
            "array"
          ],
          "default": [],
          "description": "Specifies the variable definition files to use."
        }
      }
    }
  },
  "dependencies": {
    "is-color": "^1.0.2",
    "less": "^4.0.0",
    "postcss": "^8.2.1",
    "query-ast": "^1.0.3",
    "scss-parser": "^1.0.4"
  },
  "devDependencies": {
    "@types/is-color": "^1.0.0",
    "@types/node": "^12.12.0",
    "@types/scss-parser": "^1.0.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^4.0.2"
  }
}
